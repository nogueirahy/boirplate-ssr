{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clear": "rm -rf .tmp .cache build",
    "build:client": "BABEL_ENV=client parcel build src/client/index.html -d build/static --public-url /static --no-cache  --no-source-maps",
    "build:server": "BABEL_ENV=server parcel build src/server/index.js -d build -t node --no-minify --no-cache --no-source-maps",
    "build": "yarn clear && concurrently 'yarn build:client' 'yarn build:server'",
    "serve": "NODE_ENV=production node build/index.js",
    "dev:client": "BABEL_ENV=client parcel src/client/index.html --out-dir .tmp --out-file index.html --public-url /static/ --no-cache",
    "dev:server": "nodemon --exec 'BABEL_ENV=server babel-node src/server'",
    "dev": "concurrently 'yarn dev:client' 'yarn dev:server'"
  },
  "dependencies": {
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.5",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "typescript": "^4.0.3"
  }
}
